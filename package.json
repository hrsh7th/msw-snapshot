{
  "name": "msw-snapshot",
  "version": "3.0.0",
  "description": "Transparently create an API cache for testing.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "typings": "./dist/dts/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run test:jest && npm run test:tsc",
    "test:jest": "start-server-and-test 'npm run server' 3000 'jest --runInBand'",
    "test:tsc": "tsc --noEmit",
    "server": "ts-node ./server.ts",
    "build": "npm run build:cjs && npm run build:esm && npm run build:dts",
    "build:dts": "tsc --project ./tsconfig.build.json --outDir ./dist/dts --sourceMap --emitDeclarationOnly",
    "build:cjs": "tsc --project ./tsconfig.build.json --outDir ./dist/cjs --sourceMap --module commonjs",
    "build:esm": "tsc --project ./tsconfig.build.json --outDir ./dist/esm --sourceMap --module esnext",
    "prepublishOnly": "npm run build && npm run test"
  },
  "author": "hrsh7th",
  "homepage": "https://github.com/hrsh7th/msw-snapshot",
  "repository": {
    "type": "git",
    "url": "https://github.com/hrsh7th/msw-snapshot.git"
  },
  "license": "MIT",
  "keywords": [
    "msw",
    "mswjs",
    "mock",
    "api",
    "snapshot"
  ],
  "peerDependencies": {
    "msw": "^2.0.0"
  },
  "devDependencies": {
    "@fastify/compress": "^6.5.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/multipart": "^8.0.0",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "fastify": "^4.24.3",
    "jest": "^29.7.0",
    "msw": "^2.0.3",
    "start-server-and-test": "^2.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
